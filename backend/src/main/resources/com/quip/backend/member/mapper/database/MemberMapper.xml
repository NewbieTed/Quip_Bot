<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quip.backend.member.mapper.database.MemberMapper">

    <resultMap id="MemberResultMap" type="com.quip.backend.member.model.Member">
        <id property="id" column="id"/>
        <result property="memberName" column="member_name"/>
        <result property="createdBy" column="created_by"/>
        <result property="updatedBy" column="updated_by"/>
        <result property="createdAt" column="created_at"/>
        <result property="updatedAt" column="updated_at"/>
    </resultMap>

    <select id="findById" parameterType="long" resultType="com.quip.backend.member.model.Member">
        SELECT id, member_name, created_by, updated_by, created_at, updated_at
        FROM member
        WHERE id = #{id}
    </select>
</mapper>
